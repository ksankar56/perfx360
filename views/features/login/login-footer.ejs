<script>
    $(function () {
        var body = $('body');
        var backgrounds = [
            'url(<%= locals.context%>app-assets/images/backgrounds/bg-2f.jpg) no-repeat center center fixed',
            'url(<%= locals.context%>app-assets/images/backgrounds/bg-2d.jpg) no-repeat center center fixed',
            'url(<%= locals.context%>app-assets/images/backgrounds/bg-2e.jpg) no-repeat center center fixed'];
        var current = 0;

        function nextBackground() {
            body.css(
                'background',
                backgrounds[current = ++current % backgrounds.length]);

            setTimeout(nextBackground, 20000);
        }
        setTimeout(nextBackground, 20000);
        body.css('background', backgrounds[0]);
    });
</script>