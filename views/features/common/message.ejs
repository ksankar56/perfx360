<% if (locals.err) { %>
<div class="alert alert-icon-left  alert-arrow-left alert-danger alert-dismissible fade in mb-2" role="alert">
    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
    </button>
    <%= err.message %>
</div>
<% } else if (locals.status) { %>
<div class="alert alert-icon-left  alert-arrow-left alert-success alert-dismissible fade in mb-2" role="alert">
    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
    </button>
    <%= status.message %>
</div>
<% }
%>