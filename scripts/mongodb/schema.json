CREATE TABLE perfx360.user (
  id MEDIUMINT NOT NULL AUTO_INCREMENT,
  userName Varchar(45),
  firstName Varchar(45),
  lastName Varchar(45),
  title varchar(30),
  age Number,
  Role: [
  {
    id objectid ("random alphanumeric"),
    type char(1),
  }
  PRIMARY KEY (id)
)


